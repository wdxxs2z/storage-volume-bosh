---
name: nfs_broker

description: "This is nfs_driver service broker"

templates:
  nfs_volume_broker_ctl.erb: /bin/nfs_volume_broker_ctl

packages:
  - pid_utils
  - nfsbroker

properties:
  nfs_volume_broker.configPath:
    description: "config director to store config info"
    default: "/var/vcap/data/nfs_volume_broker/config"
  nfs_volume_broker.dataDir:
    description: "[REQUIRED] - Broker's state will be stored here to persist across reboots"
    default: "/var/vcap/data/nfs_volume_broker/data"
  nfs_volume_broker.debugAddr:
    description: "host:port for serving pprof debugging info"
  nfs_volume_broker.defaultMountPath:
    description: "local director to mount within"
    default: "/tmp/share"
  nfs_volume_broker.displayName:
    description: "display the volume service broker name"
    default: "NFS Volume Support"
  nfs_volume_broker.imageUrl:
    description: "base64 code image or image url"
    default: 
  nfs_volume_broker.listenAddr:
    description: "host:port to serve nfs service broker function"
    default: "0.0.0.0:8980"
  nfs_volume_broker.logLevel:
    description: "log level: debug, info, error or fatal"
    default: "info"
  nfs_volume_broker.username:
    description: "basic auth username to verify on incoming requests"
    default: "admin"
  nfs_volume_broker.password:
    description: "basic auth password to verify on incoming requests"
    default: "admin"
  nfs_volume_broker.planDesc:
    description: "description of the service plan to register with cloud controller"
    default: "Free NFS Volume Storage Plan"
  nfs_volume_broker.planId:
    description: "guid to the nfs service plan"
    default: ""
  nfs_volume_broker.planName:
    description: "name to the nfs service plan"
    default: "free"
  nfs_volume_broker.remoteInfo:
    description: "host for nfs remoteInfo server"
    default: ""
  nfs_volume_broker.remoteMount:
    description: "if v3,the remoteMount is full director:/var/vcap/store/shared,if v4,the default is '/',you need add shared sub director such as /shared."
    default: "/shared"
  nfs_volume_broker.serviceId:
    description: "id to nfs service"
    default: ""
  nfs_volume_broker.serviceName:
    description: "name to registrer with cloud controller"
    default: "nfs"
  nfs_volume_broker.version:
    description: "version number for nfs server"
    default: 4
